<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Index">

    <ImageButton
        android:id="@+id/btnpersonal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="11dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="11dp"
        android:layout_marginStart="16dp"
        android:soundEffectsEnabled="false"
        app:layout_constraintEnd_toStartOf="@+id/btnestudiante"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnestudiante"
        app:srcCompat="@drawable/personal"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/btnestudiante"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="145dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnpersonal"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/alumno"
        tools:ignore="MissingConstraints" />
</android.support.constraint.ConstraintLayout>